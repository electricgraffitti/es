<h1>Listing gamerooms</h1>

<table>
  <tr>
    <th>Game</th>
    <th>Student</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @gamerooms.each do |gameroom| %>
  <tr>
    <td><%= gameroom.game_id %></td>
    <td><%= gameroom.student_id %></td>
    <td><%= link_to 'Show', gameroom %></td>
    <td><%= link_to 'Edit', edit_gameroom_path(gameroom) %></td>
    <td><%= link_to 'Destroy', gameroom, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Gameroom', new_gameroom_path %>
